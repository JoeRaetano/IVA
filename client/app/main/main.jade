extends ./layout.jade

block content
  <!-- Default box -->
  .box
    .box-header.with-border
      h3.box-title This is the home page
    .box-body
      Start Create your amazing application!
    form(name='fileForm', enctype='multipart/form-data', action='/api/file', method='post')
      input(type='file', name='userFile', ng-model='userFile')
      button.btn.btn-primary(type='submit',value = 'upload File')
        i.fa.fa-plus-square-o
        |  Upload
    .box-footer
      Footer
